<!DOCTYPE html>
<html>
<head>
  <base href="/">
  <meta charset="utf-8">
  <title>TLM Challenge</title>

  <link href="favicon.png" rel="shortcut icon" type="image/png">

  <!-- CSS -->
  <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,900' rel='stylesheet' type='text/css'><!-- Styles -->
  <link href="/css/loader.css" rel="stylesheet" type="text/css">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link rel="stylesheet" href="/css/font-awesome.min.css">
  <link href="/css/style.css" rel="stylesheet" type="text/css">

  <!--[if lt IE 9]>
  <link rel="stylesheet" type="text/css" href="/css/ie.css" />
  <![endif]-->
  <!-- Javascript -->
  <script src="/javascripts/jquery.js"></script>
  <script src="/Semantic-UI-CSS-master/semantic.js"></script>

</head>

<body>
<div class="preloader">
  <div class="loading">
    <h2>
      Loading...
    </h2>
    <span class="progress"></span>
  </div>
</div>

<div class="wrapper">
  <ul class="scene unselectable" data-friction-x="0.1" data-friction-y="0.1" data-scalar-x="25" data-scalar-y="15" id="scene">
    <li class="layer" data-depth="0.00">
    </li>
    <!-- BG -->

    <li class="layer" data-depth="0.10">
      <div class="background">
      </div>
    </li>

    <!-- Hero -->

    <li class="layer" data-depth="0.20">
      <div class="title">
        <h2>
          TLM Challenge
        </h2>
        <span class="line"></span>
      </div>
    </li>
    <!-- Flakes -->

    <li class="layer" data-depth="0.40">
      <div class="depth-1 flake1">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-1 flake2">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-1 flake3">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-1 flake4">
        <img alt="flake" src="/images/suriken.gif">
      </div>
    </li>

    <li class="layer" data-depth="0.50">
      <div class="depth-2 flake1">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-2 flake2">
        <img alt="flake" src="/images/suriken.gif">
      </div>
    </li>

    <li class="layer" data-depth="0.60">
      <div class="depth-3 flake1">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-3 flake2">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-3 flake3">
        <img alt="flake" src="/images/suriken.gif">
      </div>

      <div class="depth-3 flake4">
        <img alt="flake" src="/images/suriken.gif">
      </div>
    </li>

    <li class="layer" data-depth="0.80">
      <div class="depth-4">
        <img alt="flake" src="/images/flakes/depth4/flakes.png">
      </div>
    </li>

    <li class="layer" data-depth="1.00">
      <div class="depth-5">
        <img alt="flake" src="/images/flakes/depth5/flakes.png">
      </div>
    </li>
  </ul>
  <div class="contact" style="margin-bottom: 20%">
    <a href="/game"><img src="/images/ninjaHead.png" class="hoverMenu"></a>
    <p class="sub-title" data-depth="0.30">
      Des noms comme vous en avez jamais vue!
    </p>
  </div>
</div>

<!-- Javascript -->
<script src="/javascripts/plugins.js"></script>
<script src="/javascripts/jquery.countdown.min.js"></script>
<script src="/javascripts/main.js"></script>

</body>
</html>